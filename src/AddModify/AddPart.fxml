            <Label fx:id="IDAddParLabel" text="ID" />
            <Label fx:id="NameAddParLabel" text="Name" />
            <Label fx:id="InvAddParLabel" text="Inv" />
            <Label fx:id="PriceAddParLabel" text="Price/Cost" />
            <Label fx:id="MaxAddParLabel" text="Max" />

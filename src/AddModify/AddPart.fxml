            <TextField fx:id="CompNamAddParTB" onAction="#CompNamAddParTB" prefHeight="27.0" prefWidth="145.0" />
         </children>
      </VBox>
      <Button fx:id="SaveParButton" layoutX="299.0" layoutY="416.0" mnemonicParsing="false" onAction="#SavePar" text="Save" />
      <Button fx:id="CancelParButton" layoutX="379.0" layoutY="416.0" mnemonicParsing="false" onAction="#AddPartMain" text="Cancel" />
